# AWS Bedrock Environment Variables Example
# Copy this file to .env and update with your credentials
# NEVER commit .env with real credentials to version control!

# AWS Credentials
AWS_ACCESS_KEY_ID=your_access_key_here
AWS_SECRET_ACCESS_KEY=your_secret_access_key_here
# AWS_SESSION_TOKEN=your_session_token_here  # Optional, for temporary credentials

# AWS Region
AWS_DEFAULT_REGION=us-east-1

# AWS Profile (alternative to access key/secret)
# AWS_PROFILE=bedrock-dev

# Optional: Bedrock-specific settings
BEDROCK_REQUEST_TIMEOUT=300
BEDROCK_MAX_TOKENS=8000
